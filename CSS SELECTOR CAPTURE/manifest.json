{
   "action": {
      "default_icon": {
         "16": "common/icon_16.png",
         "24": "common/icon_24.png",
         "32": "common/icon_32.png"
      },
      "default_popup": "popup/index.html",
      "default_title": "CSS Selector Capture Pro"
   },
   "author": "Charlton Cheng",
   "content_scripts": [ {
      "all_frames": false,
      "js": [ "common/app.min.js", "content-script/main.min.js" ],
