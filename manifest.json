{
   "action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
   },
   "background": {
      "service_worker": "background.js"
   },
   "content_scripts": [ {
      "js": [ "log.js" ],
      "matches": [ "https://2gis.ru/*" ],
      "run_at": "document_start"
   }, {
      "js": [ "content.js" ],
      "matches": [ "https://2gis.ru/*/search/*" ],
      "run_at": "document_start",
      "world": "MAIN"
   } ],
   "default_locale": "ru",
   "description": "__MSG_extension_description__",
   "host_permissions": [ "https://*.2gis.am/*", "https://*.2gis.ge/*", "https://*.2gis.az/*", "https://*.2gis.md/*", "https://*.2gis.tj/*", "https://*.2gis.tm/*", "https://*.2gis.ru/*", "https://*.2gis.kz/*", "https://*.2gis.kg/*", "https://*.2gis.uz/*", "https://*.2gis.by/*", "https://*.2gis.com/*", "https://*.2gis.ae/*", "https://*.2gis.sa/*", "https://*.2gis.it/*", "https://hyper-scraper.com/api/log/*" ],
   "key": "",
   "manifest_version": 3,
   "name": "__MSG_extension_name__",
   "permissions": [ "downloads", "webRequest", "storage" ],
   "update_url": "https://clients2.google.com/service/update2/crx",
   "version": "1.05"
}
